--// hello world
local executor = identifyexecutor() 
local words = { 
        executor, 
}

if game.PlaceId == 597114278 then
    if syn or "Synapse" then 
        local event = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest 
        event:FireServer(words[math.random(#words)], "All")
    elseif
        event:FireServer(words[math.random(#words)], "All") then
    end
    wait(2)
    local words = {
    'Pirate Bay Loading'
    }

    local event = game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest
    event:FireServer(words[math.random(#words)], "All")
    wait(3)
    loadstring(game:HttpGet('https://raw.githubusercontent.com/PeasW/Pirate-Bay/main/HSDL'))()
end

