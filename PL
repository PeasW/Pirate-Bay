    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(832.099243, 99.9899826, 2317.01025, -0.999995232, 3.60536987e-08, -0.00308783771, 3.58573438e-08, 1, 6.36442437e-08, 0.00308783771, 6.35332142e-08, -0.999995232)
end)

sec4:Button("yard teleport", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(774.447144, 97.9999237, 2468.35864, -0.103519917, 3.38634329e-08, -0.994627357, -9.25605548e-08, 1, 4.36799716e-08, 0.994627357, 9.6585012e-08, -0.103519917)
